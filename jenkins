pipeline {
  agent any
  stages {
    stage('Git Checkout') {
      steps {
        sh 'echo passed'
        //git branch: 'main', url: 'https://github.com/iam-veeramalla/Jenkins-Zero-To-Hero.git'
      }
    }
     stage('Docker Build') {
      steps {
        sh 'docker build -t ngnix:latest .'
        
      }
    }
 }
}
